};

// --- CREATE THE ANIMATION LIST ---
// This list points to the frames above. 
const unsigned char* const animationFrames[] PROGMEM = {
	epd_bitmap_frame_0_delay_0,
	epd_bitmap_frame_1_delay_0,
	epd_bitmap_frame_2_delay_0,
	epd_bitmap_frame_3_delay_0,
	epd_bitmap_frame_4_delay_0,
	epd_bitmap_frame_5_delay_0,
	epd_bitmap_frame_7_delay_0
};

// Count how many frames you have
const int frameCount = 7; 

void setup() {
  display.begin(i2c_Address, true);
  display.clearDisplay();
}

void loop() {
  // Loop through every frame in the list
  for (int i = 0; i < frameCount; i++) {
    display.clearDisplay(); // Clear previous frame
    
    // Draw the current frame
    // (x, y, array_list[i], width, height, color)
    display.drawBitmap(0, 0, (const unsigned char*)pgm_read_ptr(&animationFrames[i]), 128, 64, SH110X_WHITE);
    
    display.display(); // Push to screen
    
    delay(50); // Adjust speed of animation here (lower number = faster)
  }
}
